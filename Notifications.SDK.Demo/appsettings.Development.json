{
  "AppStoreSettings": {
    "ClientId": "{APP_CLIENT_ID_IN_APPSTORE}",
    "ClientSecret": "{APP_CLIENT_SECRET_IN_APPSTORE}",
    "AppStoreUrl": "{APPSTORE_BASE_URL}"
  },
  "NotificationServiceEndpoint": {
    "BaseUrl": "{NOTIFICATION_SERVICE_BASE_URL}",
    "ApiBasePath": "/api",
    "NotificationsHubPath": "/hub/notifications"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http.HttpClient": "Warning"
    }
  }
}